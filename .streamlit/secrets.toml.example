# Template untuk Google OAuth
# Copy file ini ke secrets.toml dan isi dengan credentials Anda

[google_oauth]
client_id = "YOUR_GOOGLE_CLIENT_ID.apps.googleusercontent.com"
client_secret = "YOUR_GOOGLE_CLIENT_SECRET"
redirect_uri = "http://localhost:8501"  # Ubah ke IP server saat deploy

[postgresql]
# Database configuration (existing)
dialect = "postgresql"
host = "db"
port = 5433
database = ""
username = ""
password = ""
